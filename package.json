  "devDependencies": {
    "@kube-js/semantic-release": "1.0.7",
    "@kube-js/tscommons": "1.0.5",
    "@types/compression": "1.0.1",
    "@types/cors": "2.8.6",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.1",
    "@types/express-rate-limit": "3.3.2",
    "@types/faker": "4.1.5",
    "@types/git-rev": "0.2.0",
    "@types/helmet": "0.0.44",
    "@types/is-plain-obj": "1.1.0",
    "@types/jest": "24.0.18",
    "@types/jsonwebtoken": "8.3.3",
    "@types/mailparser": "2.4.0",
    "@types/node": "11.13.19",
    "@types/nodemailer": "6.2.1",
    "@types/promptly": "1.1.28",
    "@types/ramda": "0.26.21",
    "@types/smtp-server": "3.5.2",
    "@types/source-map-support": "0.5.0",
    "@types/stoppable": "1.1.0",
    "@types/supertest": "2.0.8",
    "@types/uuid": "3.4.5",
    "@types/winston": "2.4.4",
    "jest": "24.9.0",
    "pre-commit": "1.2.2",
    "rimraf": "3.0.0",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "ts-node-dev": "1.0.0-pre.42",
    "tslint": "5.19.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.5"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "artisan": "ts-node ./src/presenter/commander/index.ts",
    "migrate": "npm run artisan migrate",
    "rollback": "npm run artisan rollback",
    "setup": "npm run migrate && npm run seed",
    "seed": "npm run artisan seed",
    "duplication": "jscpd ./src",
    "lint": "tslint --project tsconfig.json --config tslint.json './src/**/*.ts'",
    "start": "node ./dist/presenter/express/index.js",
    "dev": "ts-node-dev --respawn --transpileOnly  ./src/presenter/express/index.ts",
    "semantic-release": "semantic-release-public-circleci-lib",
    "test": "jest --coverage --runInBand",
    "test:watch": "jest --watch --runInBand --coverage"
  },
  "pre-commit": [
    "build",
    "test",
    "lint",
    "duplication"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kube-js/kube-ts-server.git"
  },
  "keywords": [
    "rest",
    "api",
    "express",
    "typescript",
    "jwt"
  ],
  "author": "Mariusz Rajczakowski <mariuszrajczakowski@pm.me> (http://mariuszrajczakowski.me)",
