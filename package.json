    "@types/winston": "2.4.4",
    "jest": "24.7.1",
    "pre-commit": "1.2.2",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "tslint": "5.15.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.1"
