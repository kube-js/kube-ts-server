  "scripts": {
    "build": "rimraf dist && tsc",
    "db:migrate": "node ./dist/presenter/commander/index.js migrate",
    "duplication": "jscpd ./src",
    "lint": "tslint --project tsconfig.json --config tslint.json './src/**/*.ts'",
    "start": "node ./dist/presenter/express/index.js",
    "semantic-release": "semantic-release-public-circleci-app",
    "test": "jest --coverage --runInBand",
    "test:watch": "jest --watch --runInBand  --coverage"
