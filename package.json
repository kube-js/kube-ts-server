{
  "name": "@kube-js/kube-ts-server",
  "version": "0.0.0-development",
  "description": "Dokerized restful API written in typescript ready to be deployed on kubernetes cluster",
  "main": "index.js",
  "dependencies": {
    "@js-items/express": "1.0.8",
    "@js-items/foundation": "1.0.25",
    "@js-items/knex": "1.0.17",
